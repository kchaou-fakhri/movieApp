{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@utils/*": ["src/utils/*"],
      "@reduxConfig/*": ["src/reduxConfiguration/*"],
      "@routeNavigation/*": ["src/navigation/*"],
      "@contexts/*": ["src/contexts/*"],
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@constants/*": ["src/constants/*"],
      "@model/*": ["src/model/*"],
      "@service/*": ["src/service/*"]
    }
  }
}
